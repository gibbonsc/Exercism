{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch active (active F#)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-active-project",
            "program": "${fileDirname}/bin/Debug/net9.0/RunLengthEncoding.dll",
            
            "cwd": "${fileDirname}",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "justMyCode": true
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false
        },
    ]
}